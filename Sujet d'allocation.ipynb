    "print(\"\\nRendement portefeuille : \",ptf_return)\n",
    "\n",
    "#Calcul du delta de rendement\n",
    "delta_return = ptf_return - indice_return\n",
    "\n",
    "print(\"\\nDelta rendement : \",delta_return)"
